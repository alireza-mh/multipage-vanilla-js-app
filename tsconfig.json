{
  "compilerOptions": {
    "importHelpers": true,
    "noEmitHelpers": true,
    "baseUrl": "./",
    "paths": {
      "src/*": ["src/*"],
    },
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["webworker", "dom", "es2016", "es2017.object"],
    "target": "esnext",
    "module": "esnext",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "sourceMap": true
  },
  "exclude": ["node_modules"]
}
